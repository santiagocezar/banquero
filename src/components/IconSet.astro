---
import { generateSymbols, type IconifyJSON } from '../lib/icons'
interface Props {
  pack: IconifyJSON;
  names: string[];
  prefix?: string;
}

const symbols = generateSymbols(Astro.props.pack, Astro.props.names, Astro.props.prefix)
---

<svg style="display: none;">
  <defs set:html={symbols}>
  </defs>
</svg>
